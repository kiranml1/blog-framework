{
  "name": "Kefex",
  "version": "0.0.1",
  "description": "simple blog framework",
  "scripts": {
    "lib-build": "BUILD_PRERELEASE=true webpack --config webpack.lib.prod.config.js --progress --colors",
    "lib-dev": "BUILD_DEV=true webpack --config webpack.lib.config.js --progress --colors --watch",
    "lib-lint": "eslint ./src",
    "lib-karma": "karma start karma.lib.config.js",
    "lib-test": "mocha --opts tests/mocha.opts ./tests",
    "lib-test-coverage": "babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --reporter dot ./tests/**/*.spec.js",
    "lib-test-watch": "npm run lib-test -w",
    "react-build": "BUILD_PRERELEASE=true webpack --config webpack.guides.react.prod.config.js --progress --colors",
    "react-dev-server": "node server.js",
    "react-dev": "BUILD_DEV=true webpack-dev-server --config webpack.guides.react.config.js --progress --colors --content-base guides",
    "react-lint": "eslint ./guides/src",
    "react-karma": "karma start karma.react.config.js",
    "react-test": "mocha --opts ./guides/tests/mocha.opts ./guides/tests",
    "react-test-coverage": "babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --reporter dot ./__tests__/**/*.spec.js",
    "react-test-watch": "npm run react-test -w"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/jspm_packages"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "core-js"
    ]
  },
  "author": "Amruthaluri Kiran Kumar",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "core-js": "^2.4.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.0.0",
    "eslint-config-standard-jsx": "^1.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.3.1",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "ghooks": "^1.2.1",
    "immutable": "^3.8.1",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.3",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jest-cli": "^12.1.1",
    "jsdom": "^9.2.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.7.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.13.1",
    "lolex": "^1.5.0",
    "mocha": "^2.5.3",
    "node-libs-browser": "^1.0.0",
    "open-browser-webpack-plugin": "0.0.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "uglify-loader": "^1.3.0",
    "watch": "^0.18.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "with-package": "^0.2.0"
  },
  "dependencies": {
    "babel-runtime": "^6.9.0",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0"
  }
}
